{
  "name": "circle-concierge",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./www",
    "start:dev": "yarn install && yarn run sequelize db:migrate && yarn start",
    "test": "ava --verbose",
    "build": "npm run build-clean && npm run build-server-static && npm run build-server && npm run build-client",
    "build-client": "webpack",
    "build-server": "babel server -d dist",
    "build-server-static": "cpx \"./server/views/*\" ./dist/views & cpx \"./server/public/*\" ./dist/public",
    "build-clean": "rimraf ./dist",
    "sequelize": "sequelize",
    "env": "env",
    "db-init": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "body-parser": "~1.17.1",
    "compression": "^1.7.0",
    "connect-history-api-fallback": "^1.4.0",
    "connect-redis": "^3.3.0",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "express-session": "^1.15.5",
    "helmet": "^3.8.1",
    "hjs": "~0.0.6",
    "ioredis": "^3.1.4",
    "morgan": "~1.8.1",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.0",
    "sequelize-cli": "^3.0.0",
    "serve-favicon": "~2.4.2",
    "twitter": "^1.7.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-register": "^6.26.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.0",
    "node-sass": "^4.6.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "tap-nyan": "^1.1.0",
    "vue": "^2.5.3",
    "vue-loader": "^13.4.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.6.0"
  }
}
